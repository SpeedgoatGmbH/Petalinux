/include/ "system-conf.dtsi"
/ {
	chosen {
		bootargs = "earlycon earlyprintk clk_ignore_unused root=/dev/ram rw cma=256M";
		stdout-path = "serial0:115200n8";
	};
	mdio {
		compatible = "cdns,macb-mdio";
		reg = <0x0 0xff0b0000 0x0 0x1000>;
		clocks =  <&clk 45>, <&clk 45>, <&clk 49>;
		clock-names = "pclk", "tx_clk", "hclk";
		#address-cells = <1>;
		#size-cells = <0>;
		phy7: ethernet-phy@7 {
			reg = <7>;
			txc-skew-ps = <1800>;
			txen-skew-ps = <420>;
			txd0-skew-ps = <420>;
			txd1-skew-ps = <420>;
			txd2-skew-ps = <420>;
			txd3-skew-ps = <420>;
			rxc-skew-ps = <900>;
			rxdv-skew-ps = <420>;
			rxd0-skew-ps = <420>;
			rxd1-skew-ps = <420>;
			rxd2-skew-ps = <420>;
			rxd3-skew-ps = <420>;
		};
		phy3: ethernet-phy@3 {
			reg = <3>;
			txc-skew-ps = <1800>;
			txen-skew-ps = <420>;
			txd0-skew-ps = <420>;
			txd1-skew-ps = <420>;
			txd2-skew-ps = <420>;
			txd3-skew-ps = <420>;
			rxc-skew-ps = <900>;
			rxdv-skew-ps = <420>;
			rxd0-skew-ps = <420>;
			rxd1-skew-ps = <420>;
			rxd2-skew-ps = <420>;
			rxd3-skew-ps = <420>;
		};
	};
};

&gem0 {
	status = "okay";
	local-mac-address = [00 0a 35 00 02 90];
	phy-mode = "rgmii-id";
	phy-handle = <&phy3>;
};

&gem3 {
	status = "okay";
	local-mac-address = [00 0a 35 00 02 91];
	phy-mode = "rgmii-id";
	phy-handle = <&phy7>;
};


&sdhci1 {
	status = "okay";
	no-1-8-v;	/* for 1.0 silicon */
	disable-wp; /*wp-inverted;*/
	xlnx,mio_bank = <1>;
};

&sdhci0 {
	status = "okay";
	no-1-8-v;	/* for 1.0 silicon */
	non-removable;
	disable-wp;
	xlnx,mio_bank = <0>;
};